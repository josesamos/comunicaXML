      <!--Ha de indicar los datos del establecimiento o bien el código de establecimiento-->
      <establecimiento>
        <datosEstablecimiento>
          <tipo>{tipo}</tipo>
          <nombre>{nombre}</nombre>
          <direccion>
            <direccion>{direccion}</direccion>
            <direccionComplementaria>{direccionComplementaria}</direccionComplementaria>
            <!--Código de Municipio de 5 dígitos: [07065, 07901,...]-->
            <!--Si el país es España (ESP), el código de municipio ha de ir informado, no así el nombre del municipio-->
            <codigoMunicipio>{codigoMunicipio}</codigoMunicipio>
            <!--Si el país es distinto de España (ESP), ha de informar el nombre del municipio, no así el código de municipio-->
            <nombreMunicipio>{nombreMunicipio}</nombreMunicipio>
            <codigoPostal>{codigoPostal}</codigoPostal>
            <!--País código ISO alfa-3: [ESP, ARG, ...]-->
            <pais>{pais}</pais>
          </direccion>
        </datosEstablecimiento>
        <codigo>{codigo}</codigo>
      </establecimiento>
